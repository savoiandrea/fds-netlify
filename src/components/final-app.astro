---
import Link from "./ui/link.astro";
import team from "assets/team.jpeg";
import { Image } from "@astrojs/image/components";
---
<div class="mx-auto px-5">
    <div class="my-12 p-8 flex gap-4 rounded-lg bg-indigo-500 justify-center mx-auto max-w-xl flex-col items-center text-center">
      <h2 class="text-white text-3xl font-normal lg:tracking-tight">
        Do you want to test the final result?
      </h2>
      <div class="flex justify-center">
        <!--
        <Image src={team} alt="" width={260} height={400} />
         -->
      </div>
      <div class="flex flex-col sm:flex-row gap-3">
        <Link
          href="/project-outputs/#prototype"
          class="flex gap-1 items-center justify-center bg-black text-indigo-600"
          rel="noopener"
          style="inverted"
          >
          <!-- <Icon class="text-white w-5 h-5" name="bx:bxs-cloud-download" /> -->

          Try it!
        </Link>
      </div>
    </div>
</div>

